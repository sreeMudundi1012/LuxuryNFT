{
  "openapi": "3.0.0",
  "info": {
    "title": "LuxuryNFT",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  },
  "paths": {
    "/api/v1/sign_up": {
      "post": {
        "tags": [
          "General"
        ],
        "summary": "Sign Up",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "Username": "Sree",
                  "Email": "sree@ttt.com",
                  "Password": "test",
                  "Role": "user"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/api/v1/luxury_nft/deploy": {
      "post": {
        "tags": [
          "General"
        ],
        "summary": "Deploy NFT",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "Nonce": 6,
                  "GasPrice": 20000000000,
                  "GasLimit": 6721975,
                  "FundValue": 0
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/api/v1/sign_in": {
      "post": {
        "tags": [
          "General"
        ],
        "summary": "Sign In",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "Email": "sree@xyz.com",
                  "Password": "test"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/api/v1/luxury_nft/items": {
      "post": {
        "tags": [
          "General"
        ],
        "summary": "Add Items to DB",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "brand": "kaleido",
                  "price": 4599,
                  "tokenURI": "WBIBGFC5MYQ3170P"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/api/v1/luxury_nft/mintNFT": {
      "post": {
        "tags": [
          "General"
        ],
        "summary": "Mint NFT",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "tokenID": 7890,
                  "tokenURI": "WBIBGFC5MYQ3170P"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    }
  }
}